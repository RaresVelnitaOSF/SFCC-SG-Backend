{
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"amd": true,
		"jquery": true
	},
	"rules": {
		"func-names": 0,
		"no-new": 0,
		"no-use-before-define": 0,
		"no-underscore-dangle": 0,
		"global-require": 0,
		"no-plusplus": 0,
		"consistent-return": 0,
		"no-mixed-operators": 0,
		"max-len": 0,
		"no-empty": ["error", { "allowEmptyCatch": true }],
		"no-restricted-syntax": 0,
		"no-prototype-builtins": 0,
		"linebreak-style": 0,
        "no-fallthrough": ["error", { "commentPattern": "break[\\s\\w]*omitted" }],
        "no-console": 0,
		"no-unused-expressions": 0,
		"no-param-reassign": 0,
		"eqeqeq": 0,
		"one-var": 0,
		"vars-on-top": 0,
		"no-alert": 0,
		"guard-for-in": 0,
		"no-shadow": 0,
		"radix": 0,
		"camelcase": 0,
		"newline-per-chained-call": 0,
        "no-unused-vars": ["error", { "varsIgnorePattern": "execute|found|Resource|GetProductDetailUrl" }]
	},
	"globals": {
		"app": true,
		"pagecontext": true,
        "importPackage": true,
        "Logger": true,
        "PIPELET_NEXT": true,
        "PIPELET_ERROR": true,
        "module": true,
        "exports": true,
        "URLUtils": true,
        "pdict": true,
        "#": true
	}
}
